<resources>


    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/md_teal_500_primary</item>
        <item name="colorPrimaryDark">@color/md_teal_700</item>
        <item name="colorAccent">@color/md_teal_900</item>
    </style>

    <style name="myTheam" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/md_teal_500_primary</item>
        <item name="colorPrimaryDark">@color/md_teal_700</item>
        <item name="colorAccent">@color/md_teal_900</item>
        <item name="android:colorForeground">@color/material_grey_50</item>
    </style>

    <style name="myTheam.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="myTheam.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
